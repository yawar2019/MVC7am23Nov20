@model List<MVC7am23Nov20.Models.EmployeeModel>
@{
    ViewBag.Title = "SendMultipleValueExample2";
}

<h2>SendMultipleValueExample2</h2>

<table class="table table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.EmpId
            </td>
            <td>
                @item.EmpName
            </td>
            <td>
                @item.EmpSalary
            </td>
        </tr>
    }
</table>
